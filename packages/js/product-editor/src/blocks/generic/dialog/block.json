{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "woocommerce/product-dialog-field",
	"title": "Product dialog control",
	"category": "woocommerce",
	"description": "A reusable dialog for the product editor.",
	"keywords": [ "products", "dialog" ],
	"textdomain": "default",
	"attributes": {
		"title": {
			"type": "string",
			"__experimentalRole": "content"
		},
		"isOpen": {
			"type": "boolean",
			"default": false
		},
		"onClose": {
			"type": "function"
		}
	},
	"supports": {
		"align": false,
		"html": false,
		"multiple": true,
		"reusable": false,
		"inserter": false,
		"lock": false,
		"__experimentalToolbar": false
	},
	"editorStyle": "file:./editor.css",
	"usesContext": [ "postType" ]
}
